{
    "name": "miragesoft/yii2-user",
    "description": "Yii 2 user authentiction module",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/miragesoft/yii2-user.git",
        "issues": "https://github.com/miragesoft/yii2-user/issues"
    },
    "authors": [
        {
            "name": "Mirage Studio TH",
            "email": "miragestudioth@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "miragesoft/yii2-basicfilemanager": "*"
    },
    "autoload": {
        "psr-4": {
            "mirage\\user\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
